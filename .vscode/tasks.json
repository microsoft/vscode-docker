{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "compile",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"isBackground": true,
			"presentation": {
				"reveal": "silent"
			},
			"problemMatcher": "$tsc-watch"
		},
		{
			"type": "npm",
			"script": "lint",
			"problemMatcher": "$tslint5"
		}
	]
}
